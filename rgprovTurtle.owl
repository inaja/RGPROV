###  http://www.ecs.soton.ac.uk/rgprov#rgprov:DeleteOperation
:rgprov:DeleteOperation rdf:type owl:Class ;
                        rdfs:subClassOf :rgprov:GraphOperation ;
                        owl:disjointWith :rgprov:InsertOperation ;
                        <http://www.w3.org/ns/prov#definition> "An activity that was performed on exactly one rgprov:Graph, consisting of deleting triples found in exactly one other rgprov:Graph, to produce a third rgprov:Graph." .


###  http://www.ecs.soton.ac.uk/rgprov#rgprov:Difference
:rgprov:Difference rdf:type owl:Class ;
                   rdfs:subClassOf :rgprov:GraphOperation ;
                   owl:disjointWith :rgprov:Intersection ,
                                    :rgprov:Union ;
                   <http://www.w3.org/ns/prov#definition> "An activity that was performed on exactly two rgprov:Graphs to produce their difference." .


